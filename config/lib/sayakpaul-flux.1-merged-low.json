{
  "pipeline": {
    "pretrained_model_name_or_path": "sayakpaul/FLUX.1-merged",
    "torch_dtype": "bfloat16"
  },
  "options": {
    "enable_sequential_cpu_offload": true,
    "enable_vae_slicing": true,
    "enable_vae_tiling": true,
    "to": {
      "dtype": "float16"
    }
  },
  "generation_kwargs": {
    "standard": {
      "guidance_scale": 1.0,
      "num_inference_steps": 12,
      "max_sequence_length": 512
    },
    "hd": {
      "guidance_scale": 3.5,
      "num_inference_steps": 12,
      "max_sequence_length": 512
    }
  }
}
